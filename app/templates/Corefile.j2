{# CoreDNS Configuration File #}
{# Generated by CoreDNS Manager #}
{# Generated at: {{ generated_at }} #}

{% for zone in zones %}
{{ zone.name }} {
    hosts {
        {% for record in zone.records %}
        {{ record.ip_address }} {{ record.hostname }}.{{ zone.name }}
        {% endfor %}
        fallthrough
    }
    log
    errors
}

{% endfor %}
. {
    forward . 8.8.8.8 8.8.4.4
    log
    errors
    cache 30
}
